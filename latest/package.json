{
  "name": "clarity_addons_website",
  "version": "0.1.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/porscheinformatik/clarity-addons.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:dev": "ng serve --port 4300",
    "build": "ng build",
    "deploy:ci": "ng build --prod --outputPath=../deploy && npm run generate:404 && npm run update:versions",
    "generate:404": "cp-cli ../deploy/index.html ../deploy/404.html",
    "update:versions": "npm run update:addons_version && npm run update:clarity_version",
    "update:addons_version": "cross-var replace @ADDONS_VERSION $npm_package_dependencies__porscheinformatik_clr_addons ../deploy/ --recursive",
    "update:clarity_version": "cross-var replace @CLARITY_VERSION $npm_package_dependencies__clr_angular ../deploy/ --recursive",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "11.2.13",
    "@angular/common": "11.2.13",
    "@angular/compiler": "11.2.13",
    "@angular/core": "11.2.13",
    "@angular/forms": "11.2.13",
    "@angular/platform-browser": "11.2.13",
    "@angular/platform-browser-dynamic": "11.2.13",
    "@angular/platform-server": "11.2.13",
    "@angular/router": "11.2.13",
    "@cds/core": "5.1.1",
    "@clr/angular": "5.1.1",
    "@clr/icons": "5.1.1",
    "@clr/ui": "5.1.1",
    "@porscheinformatik/clr-addons": "10.1.7",
    "@webcomponents/custom-elements": "1.4.3",
    "@webcomponents/webcomponentsjs": "2.5.0",
    "classlist.js": "1.1.20150312",
    "core-js": "3.12.1",
    "intl": "1.2.5",
    "jump.js": "1.0.2",
    "prismjs": "1.23.0",
    "rxjs": "7.0.1",
    "tslib": "2.2.0",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1102.13",
    "@angular/cli": "11.2.13",
    "@angular/compiler-cli": "11.2.13",
    "@angular/language-service": "11.2.13",
    "@types/node": "13.13.52",
    "codelyzer": "6.0.2",
    "cp-cli": "2.0.0",
    "cross-var": "1.1.0",
    "gh-pages": "3.1.0",
    "replace": "1.2.1",
    "ts-loader": "9.1.2",
    "tslint": "6.1.3",
    "typescript": "4.1.3"
  }
}
