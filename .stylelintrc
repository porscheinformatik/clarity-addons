{
  "extends": "stylelint-config-recommended",
  "defaultSeverity": "error",
  "syntax": "scss",
  "plugins": [
    "stylelint-scss"
  ],
  "rules": {
    "no-descending-specificity": null,
    "at-rule-no-unknown": null,
    "scss/at-rule-no-unknown": true,
    "selector-type-no-unknown": [true, {
      "ignoreTypes": ["/^clr-/"]
    }],
  }
}
