{
    "name": "clr-addons",
    "version": "3.2.1",
    "private": true,
    "scripts": {
        "build:ui:css": "node-sass --include-path=node_modules --source-map=true --precision=6 --indentedSyntax=true ./src/clr-addons/themes/clr-ui/clr-ui.scss ./dist/clr-addons/styles/clr-ui.css && node-sass --include-path=node_modules --source-map=true --precision=6 --indentedSyntax=true ./src/clr-addons/themes/clr-ui-dark/clr-ui-dark.scss ./dist/clr-addons/styles/clr-ui-dark.css && node-sass --include-path=node_modules --source-map=true --precision=6 --indentedSyntax=true ./src/clr-addons/themes/mvap/mvap-theme.scss ./dist/clr-addons/styles/clr-addons-mvap.css && node-sass --include-path=node_modules --source-map=true --precision=6 --indentedSyntax=true ./src/clr-addons/themes/vu3/vu3-theme.scss ./dist/clr-addons/styles/clr-addons-vu3.css",
        "build:ui:prefix": "postcss ./dist/clr-addons/styles/clr-ui.css -o ./dist/clr-addons/styles/clr-ui.css && postcss ./dist/clr-addons/styles/clr-ui-dark.css -o ./dist/clr-addons/styles/clr-ui-dark.css && postcss ./dist/clr-addons/styles/clr-addons-mvap.css -o ./dist/clr-addons/styles/clr-addons-mvap.css && postcss ./dist/clr-addons/styles/clr-addons-vu3.css -o ./dist/clr-addons/styles/clr-addons-vu3.css",
        "build:ui:src": "cpy --parents --cwd=src/clr-addons **/*.scss ../../dist/clr-addons/src/",
        "build:ui:fonts": "cpy --parents --cwd=src/clr-addons/themes fonts/**/*.* ../../../dist/clr-addons/",
        "build:ui:optimize": "csso ./dist/clr-addons/styles/clr-ui.css ./dist/clr-addons/styles/clr-ui.min.css --map file && csso ./dist/clr-addons/styles/clr-ui-dark.css ./dist/clr-addons/styles/clr-ui-dark.min.css --map file && csso ./dist/clr-addons/styles/clr-addons-mvap.css ./dist/clr-addons/styles/clr-addons-mvap.min.css --map file && csso ./dist/clr-addons/styles/clr-addons-vu3.css ./dist/clr-addons/styles/clr-addons-vu3.min.css --map file",
        "build:ui": "npm-run-all build:ui:css build:ui:prefix build:ui:src build:ui:optimize build:ui:fonts",
        "build:angular:package": "cpy ./npm/clr-addons/package.json ./npm/clr-addons/README.md ./src/clr-addons/ && cross-var replace @VERSION $npm_package_version ./src/clr-addons/package.json",
        "build:angular:ngpackagr": "ng build clr-addons",
        "build:angular": "npm-run-all build:angular:package build:angular:ngpackagr",
        "build": "npm-run-all clean build:ui build:angular",
        "ng": "ng",
        "start": "npm-run-all build:ui:css build:ui:prefix serve:dev",
        "serve:dev": "ng serve dev",
        "clean": "shx rm -rf dist",
        "test:format": "prettier --list-different ./**/*.{js,json,md,ts,scss}",
        "test:lint": "tslint -p . -c tslint.json \"src/**/*.ts\"",
        "test": "ng test clr-addons",
        "test:watch": "ng test clr-addons --watch",
        "test:travis": "npm-run-all build:ui test:format test:lint test build",
        "format:fix": "pretty-quick --staged",
        "lint:fix": "tslint -p . -c tslint.json --fix \"src/**/*.ts\" -e \"**/node_modules/**\"",
        "publish": "npm publish dist/clr-addons --access public"
    },
    "dependencies": {
        "@angular/animations": "7.0.0",
        "@angular/common": "7.0.0",
        "@angular/compiler": "7.0.0",
        "@angular/core": "7.0.0",
        "@angular/forms": "7.0.0",
        "@angular/platform-browser": "7.0.0",
        "@angular/platform-browser-dynamic": "7.0.0",
        "@angular/router": "7.0.0",
        "@clr/angular": "1.0.0",
        "@clr/icons": "1.0.0",
        "@clr/ui": "1.0.0",
        "@webcomponents/custom-elements": "1.1.0",
        "prismjs": "1.6.0",
        "rxjs": "6.3.3",
        "web-animations-js": "2.3.1",
        "zone.js": "0.8.26"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "0.10.3",
        "@angular-devkit/build-ng-packagr": "0.10.3",
        "@angular-devkit/core": "7.0.3",
        "@angular/cli": "7.0.3",
        "@angular/compiler-cli": "7.0.0",
        "@types/jasmine": "2.8.6",
        "@types/jasmine-expect": "3.8.1",
        "@types/jasmine-matchers": "0.2.30",
        "@types/jasminewd2": "2.0.3",
        "@types/node": "8.9.5",
        "autoprefixer": "6.7.7",
        "cpy-cli": "1.0.1",
        "cross-var": "1.1.0",
        "csso-cli": "1.1.0",
        "del-cli": "1.1.0",
        "husky": "1.0.0-rc.8",
        "jasmine-core": "2.99.1",
        "jasmine-expect": "3.8.3",
        "jasmine-spec-reporter": "4.2.1",
        "karma": "3.0.0",
        "karma-chrome-launcher": "2.2.0",
        "karma-coverage-istanbul-reporter": "1.4.2",
        "karma-edge-launcher": "0.4.2",
        "karma-firefox-launcher": "1.1.0",
        "karma-htmlfile-reporter": "0.3.7",
        "karma-ie-launcher": "1.0.0",
        "karma-jasmine": "1.1.1",
        "karma-jasmine-html-reporter": "0.2.2",
        "karma-jasmine-matchers": "3.7.0",
        "karma-mocha-reporter": "2.2.5",
        "karma-notify-reporter": "1.0.1",
        "karma-safari-launcher": "1.0.0",
        "karma-sauce-launcher": "1.2.0",
        "karma-scss-preprocessor": "3.0.0",
        "lite-server": "2.3.0",
        "ng-packagr": "4.3.0",
        "node-sass": "4.8.3",
        "npm-run-all": "4.1.5",
        "postcss-cli": "5.0.0",
        "prettier": "1.12.1",
        "pretty-quick": "1.6.0",
        "replace": "1.0.0",
        "shx": "0.3.0",
        "tsickle": "0.33.0",
        "tslib": "1.9.0",
        "tslint": "5.11.0",
        "tslint-config-prettier": "1.13.0",
        "tslint-defocus": "2.0.5",
        "typescript": "3.1.3"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run format:fix"
        }
    }
}
