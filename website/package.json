  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:dev": "ng serve --port 4300",
    "build": "ng build",
    "deploy:ci": "npm run update:versions && ng build --configuration production --outputPath=./dist/deploy && npm run generate:404",
    "generate:404": "cp-cli ./dist/deploy/index.html ./dist/deploy/404.html",
    "update:versions": "node ./replace-versions.js",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "env": "env"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "13.3.11",
    "@angular/common": "13.3.11",
    "@angular/compiler": "13.3.11",
