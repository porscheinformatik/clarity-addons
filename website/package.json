  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/porscheinformatik/clarity-addons.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:dev": "ng serve --port 4300",
    "build": "ng build",
    "deploy:ci": "npm run update:versions && ng build --configuration production --output-path=./dist/deploy && npm run generate:404",
    "generate:404": "cp-cli ./dist/deploy/index.html ./dist/deploy/404.html",
    "update:versions": "node ./replace-versions.js",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "env": "env"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "14.2.8",
    "@angular/common": "14.2.8",
    "@angular/compiler": "14.2.8",
    "@angular/core": "14.2.8",
    "@angular/forms": "14.2.8",
    "@angular/platform-browser": "14.2.8",
    "@angular/platform-browser-dynamic": "14.2.8",
    "@angular/platform-server": "14.2.8",
    "@angular/router": "14.2.8",
    "@cds/core": "6.2.2",
    "@clr/angular": "13.10.0",
    "@clr/icons": "13.0.2",
    "@clr/ui": "13.10.0",
    "@porscheinformatik/clr-addons": "../dist/clr-addons",
    "@webcomponents/custom-elements": "1.5.1",
    "@webcomponents/webcomponentsjs": "2.7.0",
    "core-js": "3.26.0",
    "intl": "1.2.5",
    "jump.js": "1.0.2",
    "prismjs": "1.29.0",
    "rxjs": "7.8.0",
    "tslib": "2.4.1",
    "zone.js": "0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "14.2.8",
    "@angular/cli": "14.2.8",
    "@angular/compiler-cli": "14.2.8",
    "@angular/language-service": "14.2.8",
    "@types/node": "18.11.9",
    "codelyzer": "6.0.2",
    "cp-cli": "2.0.0",
    "gh-pages": "4.0.0",
    "ts-loader": "9.4.1",
    "tslint": "6.1.3",
    "typescript": "4.8.4"
  }
}
