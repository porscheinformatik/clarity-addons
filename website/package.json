{
  "name": "clarity_addons_website",
  "version": "0.1.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/porscheinformatik/clarity-addons.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --ssl",
    "start:dev": "ng serve --port 4300 --ssl",
    "build": "ng build",
    "deploy:ci": "npm run update:versions && ng build --configuration production --output-path=./dist/deploy && npm run generate:404",
    "generate:404": "cp-cli ./dist/deploy/index.html ./dist/deploy/404.html",
    "update:versions": "node ./replace-versions.js",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "env": "env"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "18.2.5",
    "@angular/cdk": "18.2.5",
    "@angular/common": "18.2.5",
    "@angular/compiler": "18.2.5",
    "@angular/core": "18.2.5",
    "@angular/forms": "18.2.5",
    "@angular/platform-browser": "18.2.5",
    "@angular/platform-browser-dynamic": "18.2.5",
    "@angular/platform-server": "18.2.5",
    "@angular/router": "18.2.5",
    "@cds/core": "^6.13.0",
    "@clr/angular": "^17.3.0",
    "@clr/ui": "^17.3.0",
    "@kolkov/angular-editor": "2.1.0",
    "@porscheinformatik/clr-addons": "file:../dist/clr-addons",
    "@webcomponents/custom-elements": "1.6.0",
    "@webcomponents/webcomponentsjs": "2.8.0",
    "angular-calendar": "0.31.1",
    "core-js": "3.39.0",
    "date-fns": "2.30.0",
    "intl": "1.2.5",
    "jump.js": "1.0.2",
    "prismjs": "1.29.0",
    "rxjs": "7.8.1",
    "tslib": "2.8.1",
    "zone.js": "0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "18.2.5",
    "@angular/cli": "18.2.5",
    "@angular/compiler-cli": "18.2.5",
    "@angular/language-service": "18.2.5",
    "@types/node": "22.10.1",
    "codelyzer": "6.0.2",
    "cp-cli": "2.0.0",
    "gh-pages": "6.2.0",
    "ts-loader": "9.5.1",
    "typescript": "5.5.4"
  }
}
