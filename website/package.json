    "@angular/language-service": "13.3.11",
    "@types/node": "16.11.39",
    "codelyzer": "6.0.2",
    "cp-cli": "2.0.0",
    "gh-pages": "4.0.0",
    "ts-loader": "9.3.1",
    "tslint": "6.1.3",
    "typescript": "4.6.4"
  }
